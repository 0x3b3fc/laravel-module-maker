{
    "name": "phpsamurai/laravel-module-maker",
    "description": "A Laravel package for generating modular HMVC structures with artisan commands",
    "keywords": [
        "laravel",
        "hmvc",
        "modular",
        "generator",
        "artisan"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "PhpSamurai\\LaravelModuleMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSamurai\\LaravelModuleMaker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpSamurai\\LaravelModuleMaker\\ModuleMakerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
